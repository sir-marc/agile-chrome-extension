{
  "name": "Agile",
  "description": "Help keep your times in line",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/enso-16x16.png",
      "32": "/images/enso-32x32.png",
      "48": "/images/enso-48x48.png",
      "128": "/images/enso-128x128.png"
    }
  },
  "icons": {
    "16": "/images/enso-16x16.png",
    "32": "/images/enso-32x32.png",
    "48": "/images/enso-48x48.png",
    "128": "/images/enso-128x128.png"
  },
  "options_page": "options.html"
}
